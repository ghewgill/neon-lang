--IMPORT math

print(str(0))
--= 0

print(str(1))
print(str(-1))
--= 1
--= -1

LET a: Number := 0.1
LET b: Number := a + 0.2
print(b)
--= 0.3

print(str(9.999999999999999999999999999999999))
print(str(-9.999999999999999999999999999999999))
--= 9.999999999999999999999999999999999
--= -9.999999999999999999999999999999999

print(str(123.45))
print(str(-123.45))
--= 123.45
--= -123.45

print(str(1000))
print(str(-1000))
--= 1000
--= -1000

print(str(9999999999999999999999999999999999))
print(str(-9999999999999999999999999999999999))
--= 9999999999999999999999999999999999
--= -9999999999999999999999999999999999

print(str(1e35))
print(str(-1e35))
--= 100000000000000000000000000000000000
--= -100000000000000000000000000000000000

print(str(1.2345e40))
print(str(-1.2345e40))
--= 12345000000000000000000000000000000000000
--= -12345000000000000000000000000000000000000

print(str(0.1))
print(str(-0.1))
--= 0.1
--= -0.1

print(str(0.001))
print(str(-0.001))
--= 0.001
--= -0.001

print(str(1e-34))
print(str(-1e-34))
--= 0.0000000000000000000000000000000001
--= -0.0000000000000000000000000000000001

print(str(0.9999999999999999999999999999999999))
print(str(-0.9999999999999999999999999999999999))
--= 0.9999999999999999999999999999999999
--= -0.9999999999999999999999999999999999

print(str(1e-35))
print(str(-1e-35))
--= 0.00000000000000000000000000000000001
--= -0.00000000000000000000000000000000001

print(str(1.2345e-40))
