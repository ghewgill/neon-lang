ASSERT md5("abcdefghijklmnopqrstuvwxyz") = HEXBYTES "c3fcd3d76192e4007dfb496cca67e13b"
ASSERT md5("ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789") = HEXBYTES "d174ab98d277d9f5a5611c2c9f419d9f"
ASSERT md5("12345678901234567890123456789012345678901234567890123456789012345678901234567890") = HEXBYTES "57edf4a22be3c955ac49da2e2107b67a"

