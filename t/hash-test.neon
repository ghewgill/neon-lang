IMPORT hash
IMPORT string

FUNCTION md5(s: String): Bytes RETURN hash.md5Raw(s.toBytes()) END FUNCTION
FUNCTION sha1(s: String): Bytes RETURN hash.sha1Raw(s.toBytes()) END FUNCTION
FUNCTION sha256(s: String): Bytes RETURN hash.sha256Raw(s.toBytes()) END FUNCTION
FUNCTION sha512(s: String): Bytes RETURN hash.sha512Raw(s.toBytes()) END FUNCTION
FUNCTION sha3(s: String): Bytes RETURN hash.sha3Raw(s.toBytes()) END FUNCTION

