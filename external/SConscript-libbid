            envwith(dpmlenv, CPPDEFINES=["T_FLOAT"]).Object("IntelRDFPMathLib20U1/LIBRARY/float128/dpml_pow_t_table", "IntelRDFPMathLib20U1/LIBRARY/float128/dpml_pow_t_table.c"),
