import os
import shutil
import sys
import tarfile

Import("env")

def envwith(env, **kwargs):
    e = env.Clone()
    e.Append(**kwargs)
    return e

bidenv = Environment()

if GetOption("clean"):
    shutil.rmtree("IntelRDFPMathLib20U1", ignore_errors=True)
elif not os.path.exists("IntelRDFPMathLib20U1/LIBRARY/makefile.mak"):
    tarfile.open("IntelRDFPMathLib20U1.tar.gz").extractall(".")
    if not env["RELEASE"]:
        if sys.platform == "win32":
            makefile = open("IntelRDFPMathLib20U1/LIBRARY/makefile.mak").read()
            makefile = makefile.replace("DEBUG=/Od /Zi", "DEBUG=/Od /Zi /MDd")
            open("IntelRDFPMathLib20U1/LIBRARY/makefile.mak", "w").write(makefile)

if not env["BID_GENERIC"]:
    if sys.platform == "win32":
        libbid = bidenv.Command("IntelRDFPMathLib20U1/LIBRARY/libbid.lib", "IntelRDFPMathLib20U1/LIBRARY/makefile.mak", "cd external/IntelRDFPMathLib20U1/LIBRARY && nmake -fmakefile.mak CC=cl GLOBAL_RND=1 GLOBAL_FLAGS=1 DBG=1")
    else:
        libbid = bidenv.Command("IntelRDFPMathLib20U1/LIBRARY/libbid.a", "IntelRDFPMathLib20U1/LIBRARY/makefile.mak", "cd external/IntelRDFPMathLib20U1/LIBRARY && make CC=gcc GLOBAL_RND=1 GLOBAL_FLAGS=1")
else:
    bidenv.Append(CPPDEFINES=["USE_COMPILER_F128_TYPE=0", "USE_COMPILER_F80_TYPE=0"])
    bidenv.Append(CPPDEFINES=["DECIMAL_GLOBAL_ROUNDING=1", "DECIMAL_GLOBAL_EXCEPTION_FLAGS=1"])
    if sys.platform == "win32":
        if not env["RELEASE"]:
            bidenv.Append(CFLAGS=["/MTd"])
        else:
            bidenv.Append(CFLAGS=["/MT"])
    else:
        bidenv.Append(CPPDEFINES=["efi2"])

    dpmlenv = Environment()
    dpmlenv.Append(CPPDEFINES=["USE_NATIVE_QUAD_TYPE=0"])
    dpmlenv.Append(CPPDEFINES=["ia32"])
    if sys.platform == "win32":
        dpmlenv.Append(CPPDEFINES=["wnt"])
        if not env["RELEASE"]:
            dpmlenv.Append(CFLAGS=["/MTd"])
        else:
            dpmlenv.Append(CFLAGS=["/MT"])
    else:
        dpmlenv.Append(CPPDEFINES=["efi2", "linux", "gcc"])

    dpml = [
        dpmlenv.Object("IntelRDFPMathLib20U1/LIBRARY/float128/dpml_ux_bid.c"),
        dpmlenv.Object("IntelRDFPMathLib20U1/LIBRARY/float128/dpml_ux_bessel.c"),
        dpmlenv.Object("IntelRDFPMathLib20U1/LIBRARY/float128/dpml_ux_cbrt.c"),
        dpmlenv.Object("IntelRDFPMathLib20U1/LIBRARY/float128/dpml_ux_erf.c"),
        dpmlenv.Object("IntelRDFPMathLib20U1/LIBRARY/float128/dpml_ux_exp.c"),
        dpmlenv.Object("IntelRDFPMathLib20U1/LIBRARY/float128/dpml_ux_int.c"),
        dpmlenv.Object("IntelRDFPMathLib20U1/LIBRARY/float128/dpml_ux_inv_hyper.c"),
        dpmlenv.Object("IntelRDFPMathLib20U1/LIBRARY/float128/dpml_ux_inv_trig.c"),
        dpmlenv.Object("IntelRDFPMathLib20U1/LIBRARY/float128/dpml_ux_lgamma.c"),
        dpmlenv.Object("IntelRDFPMathLib20U1/LIBRARY/float128/dpml_ux_log.c"),
        dpmlenv.Object("IntelRDFPMathLib20U1/LIBRARY/float128/dpml_ux_mod.c"),
        dpmlenv.Object("IntelRDFPMathLib20U1/LIBRARY/float128/dpml_ux_powi.c"),
        dpmlenv.Object("IntelRDFPMathLib20U1/LIBRARY/float128/dpml_ux_pow.c"),
        dpmlenv.Object("IntelRDFPMathLib20U1/LIBRARY/float128/dpml_ux_sqrt.c"),
        dpmlenv.Object("IntelRDFPMathLib20U1/LIBRARY/float128/dpml_ux_trig.c"),
        dpmlenv.Object("IntelRDFPMathLib20U1/LIBRARY/float128/dpml_ux_ops.c"),
        dpmlenv.Object("IntelRDFPMathLib20U1/LIBRARY/float128/dpml_ux_ops_64.c"),
        dpmlenv.Object("IntelRDFPMathLib20U1/LIBRARY/float128/dpml_four_over_pi.c"),
        dpmlenv.Object("IntelRDFPMathLib20U1/LIBRARY/float128/dpml_exception.c"),
        dpmlenv.Object("IntelRDFPMathLib20U1/LIBRARY/float128/sqrt_tab_t.c"),
    ]
    if sys.platform == "win32":
        dpml.extend([
            envwith(dpmlenv, CPPDEFINES=["T_FLOAT", "ASINH"]).Object("IntelRDFPMathLib20U1/LIBRARY/float128/dpml_asinh_t", "IntelRDFPMathLib20U1/LIBRARY/float128/dpml_asinh.c"),
            envwith(dpmlenv, CPPDEFINES=["T_FLOAT", "ACOSH"]).Object("IntelRDFPMathLib20U1/LIBRARY/float128/dpml_acosh_t", "IntelRDFPMathLib20U1/LIBRARY/float128/dpml_asinh.c"),
            envwith(dpmlenv, CPPDEFINES=["T_FLOAT", "CBRT", "BUILD_FILE_NAME=dpml_cbrt_t_table.c"]).Object("IntelRDFPMathLib20U1/LIBRARY/float128/dpml_cbrt_t", "IntelRDFPMathLib20U1/LIBRARY/float128/dpml_cbrt.c"),
            envwith(dpmlenv, CPPDEFINES=["T_FLOAT", "ERF", "BUILD_FILE_NAME=dpml_erf_t.h"]).Object("IntelRDFPMathLib20U1/LIBRARY/float128/dpml_erf_t", "IntelRDFPMathLib20U1/LIBRARY/float128/dpml_erf.c"),
            envwith(dpmlenv, CPPDEFINES=["T_FLOAT", "ERFC", "BUILD_FILE_NAME=dpml_erf_t.h"]).Object("IntelRDFPMathLib20U1/LIBRARY/float128/dpml_erfc_t", "IntelRDFPMathLib20U1/LIBRARY/float128/dpml_erf.c"),
            envwith(dpmlenv, CPPDEFINES=["T_FLOAT", "EXPM1", "USE_CONTROL87"]).Object("IntelRDFPMathLib20U1/LIBRARY/float128/dpml_expm1_t", "IntelRDFPMathLib20U1/LIBRARY/float128/dpml_expm1.c"),
            envwith(dpmlenv, CPPDEFINES=["T_FLOAT", "EXP10", "USE_CONTROL87"]).Object("IntelRDFPMathLib20U1/LIBRARY/float128/dpml_exp10_t", "IntelRDFPMathLib20U1/LIBRARY/float128/dpml_exp.c"),
            envwith(dpmlenv, CPPDEFINES=["T_FLOAT", "EXP2", "USE_CONTROL87"]).Object("IntelRDFPMathLib20U1/LIBRARY/float128/dpml_exp2_t", "IntelRDFPMathLib20U1/LIBRARY/float128/dpml_exp.c"),
            envwith(dpmlenv, CPPDEFINES=["T_FLOAT", "DO_LGAMMA", "HACK_GAMMA_INLINE=0", "BUILD_FILE_NAME=dpml_lgamma_t.h"]).Object("IntelRDFPMathLib20U1/LIBRARY/float128/dpml_lgamma_t", "IntelRDFPMathLib20U1/LIBRARY/float128/dpml_lgamma.c"),
            envwith(dpmlenv, CPPDEFINES=["T_FLOAT", "LOG1P"]).Object("IntelRDFPMathLib20U1/LIBRARY/float128/dpml_log1p_t", "IntelRDFPMathLib20U1/LIBRARY/float128/dpml_log.c"),
